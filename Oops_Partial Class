using System;

namespace Test
{
    partial class MyClass
    {
        public void M1()
        {
            Console.WriteLine("M1");
        }
    }
    partial class MyClass
    {
        public void M2()
        {
            Console.WriteLine("M2");
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            MyClass m = new MyClass();
            m.M1();
            m.M2();
        }
    }
}



===
M1
M2
===
