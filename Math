  class Program
  {
    static void Main(string[] args)
    {
      Console.WriteLine(Math.Sqrt(64));  
     }
  }
}
==
8
==

using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      Console.WriteLine(Math.Abs(-4.7));  
     }
  }
}
==
4.7
==

using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      Console.WriteLine(Math.Round(9.99));
     }
  }
}
